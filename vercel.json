{
    "version": 2,
    "builds": [
      { "src": "client/package.json", "use": "@vercel/next" }, 
      { "src": "api/index.ts", "use": "@vercel/node" }          
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.ts" }, 
      { "src": "/(.*)", "dest": "/client/$1" }        
    ]
  }
  